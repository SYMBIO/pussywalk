  "scripts": {
    "dev": "babel-node ./webpack/server.js",
    "build": "rm -rf build && npm run compress_ss && webpack --config ./webpack/webpack.config.prod.babel.js && npm run app_assets",

    "compress_ss": "tinypng -r -k dY76SanOkUE8SaPaTDZP-vZlJsbtEARu src/images/*/*",

    "app_assets": "npm run app_build_zip && npm run app_timestamp",
    "app_build_zip": "tar -zcvf build.zip build/ && mkdir build/app && cp build.zip build/app",
    "app_timestamp": "echo \"$(git rev-parse HEAD)\" >> build/app/update"
  },
  "dependencies": {
    "assets-loader": "^0.5.1",
    "firebase": "^4.6.0",
